{
  "builds": [
    {
      "src": "apps/client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/apps/client"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/apps/client/$1"
    }
  ],
  "outputDirectory": "dist/apps/client"
}